{
    "version": "2.0.0",
    "configurations": [
    {
    "name": "Debug ANTLR4 grammar",
    "type": "antlr-debug",
    "request": "launch",
    "input": "input.txt",
    "grammar": "analizador.g4",
    "printParseTree": true,
    "visualParseTree": true
    },
    {
    "name": "Launch Program",
    "type": "node",
    "request": "launch",
    "program": "${workspaceFolder}/index.js",
    "skipFiles": ["<node_internals>/**"]
    }
    ]
}